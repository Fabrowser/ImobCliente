@model Controle_Imob_Clientes.Models.Administrador

@{
    Layout = "~/Views/Shared/_LayoutImobClientes.cshtml";
}

<div class="card-block">
    <div class="card-header text-center bg-light mb-3 text-center h5">
        <div class="card-body">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="RegistroAdministrador" class="control-label"></label>
                    <input asp-for="RegistroAdministrador" class="form-control" />
                    <span asp-validation-for="RegistroAdministrador" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Nome" class="control-label">
                    </label>
                    <input asp-for="Nome" class="form-control" />
                    <span asp-validation-for="Nome" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Nascimento" class="control-label">
                    </label>
                    <input asp-for="Nascimento" class="form-control" />
                    <span asp-validation-for="Nascimento" class="text-danger"></span>
                </div>
                <div class="form-group text-center h4">
                    <input type="submit" value="Registrar Acadêmico"
                           class="btn btn-light" />
                    <a asp-action="Index" class="btn btn-info">
                        Retornar à listagem de Administradores
                    </a>
                </div>
            </form>
        </div>
        <div class="card-footer bg-dark text-center text-white">
            Informe os dados acima e/ou clique em um dos botões de ação
        </div>
    </div>
    @section ScriptPage {
        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");
        }
    }
